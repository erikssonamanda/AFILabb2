@model AnnonssystemMVC.Models.Ads

@{
    ViewData["Title"] = "Visa Annons";
}

<h2>Annons: @Html.DisplayFor(model => model.Ad_Id)</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Id
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Ad_Id)
        </dd>
        <dt>
            Annonsör
        </dt>
